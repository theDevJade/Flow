{
    "name": "flow-lang/flow",
    "description": "PHP bindings for the Flow programming language",
    "type": "library",
    "keywords": ["flow", "ffi", "bindings", "language"],
    "license": "MIT",
    "authors": [
        {
            "name": "Flow Language Team"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Flow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\Tests\\": "tests/"
        }
    }
}

