// Struct methods with parameters

struct Calculator {
    int val;
}

impl Calculator::add(x: int) -> int {
    return this.val + x;
}

impl Calculator::multiply(x: int) -> int {
    return this.val * x;
}

impl Calculator::compute(a: int, b: int) -> int {
    return this.val + a * b;
}

impl Calculator::sum_three(a: int, b: int, c: int) -> int {
    return this.val + a + b + c;
}

func main() -> int {
    println("Struct methods with parameters compiled!");
    return 0;
}

