// Function examples

func add(a: int, b: int) -> int {
    return a + b;
}

func multiply(x: int, y: int) -> int {
    return x * y;
}

func greet(name: string) {
    print("Hello, " + name + "!");
}

func main() {
    let sum: int = add(5, 3);
    print("5 + 3 = " + sum);
    
    let product: int = multiply(4, 7);
    print("4 * 7 = " + product);
    
    greet("World");
}

